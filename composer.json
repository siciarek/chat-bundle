{
    "name": "siciarek/chat-bundle",
    "type": "library",
    "description": "Simple chat bundle",
    "keywords": [
        "symfony2",
        "bundle",
        "chat",
        "simple chat"
    ],
    "homepage": "http://github.com/siciarek/chat-bundle.git",
    "license": "MIT",
